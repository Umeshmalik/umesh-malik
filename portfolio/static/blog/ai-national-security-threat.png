<svg width="1200" height="600" xmlns="http://www.w3.org/2000/svg">
  <!-- Background with gradient -->
  <defs>
    <linearGradient id="bgGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#000000"/>
      <stop offset="100%" stop-color="#0A0A0A"/>
    </linearGradient>
    <pattern id="grid3" width="40" height="40" patternUnits="userSpaceOnUse">
      <path d="M 40 0 L 0 0 0 40" fill="none" stroke="#1A1A1A" stroke-width="1"/>
    </pattern>
  </defs>

  <rect width="1200" height="600" fill="url(#bgGrad)"/>
  <rect width="1200" height="600" fill="url(#grid3)" opacity="0.3"/>

  <!-- Shield icon (broken) -->
  <g transform="translate(300, 250)">
    <!-- Broken shield path -->
    <path d="M 0,-100 L 60,-80 L 60,0 L 0,40 Z" fill="none" stroke="#FF4444" stroke-width="4" opacity="0.6"/>
    <path d="M 0,-100 L -60,-80 L -60,0 L 0,40 Z" fill="none" stroke="#FF4444" stroke-width="4" opacity="0.6"/>

    <!-- Crack in the middle -->
    <path d="M 0,-100 L 5,-50 L -5,-20 L 3,10 L 0,40" stroke="#FF4444" stroke-width="2.5" fill="none" stroke-linecap="round"/>

    <!-- Warning symbol inside -->
    <circle cx="0" cy="-30" r="35" fill="rgba(255,68,68,0.2)" stroke="#FF4444" stroke-width="2"/>
    <text x="0" y="-15" font-family="Inter, sans-serif" font-size="40" font-weight="700" fill="#FF4444" text-anchor="middle">!</text>
  </g>

  <!-- Biohazard and cyber symbols -->
  <g transform="translate(950, 150)">
    <!-- Biohazard symbol -->
    <circle cx="0" cy="0" r="50" fill="none" stroke="#FF4444" stroke-width="2.5" opacity="0.4"/>
    <circle cx="0" cy="0" r="12" fill="#FF4444" opacity="0.6"/>
    <circle cx="0" cy="-35" r="12" fill="none" stroke="#FF4444" stroke-width="2.5" opacity="0.6"/>
    <circle cx="30" cy="17" r="12" fill="none" stroke="#FF4444" stroke-width="2.5" opacity="0.6"/>
    <circle cx="-30" cy="17" r="12" fill="none" stroke="#FF4444" stroke-width="2.5" opacity="0.6"/>
  </g>

  <g transform="translate(950, 400)">
    <!-- Cyber attack symbol -->
    <path d="M -30,-20 L 30,-20 L 30,20 L -30,20 Z" fill="none" stroke="#C09E5A" stroke-width="2.5" opacity="0.6"/>
    <path d="M -35,-25 L -35,-35 M 35,-25 L 35,-35 M -35,25 L -35,35 M 35,25 L 35,35" stroke="#C09E5A" stroke-width="2.5" opacity="0.6"/>
    <path d="M -20,-10 L -10,-10 L -10,0 L -20,0 Z" fill="#C09E5A" opacity="0.6"/>
    <path d="M 0,-10 L 10,-10 L 10,0 L 0,0 Z" fill="#C09E5A" opacity="0.6"/>
    <path d="M 20,-10 L 30,-10 L 30,0 L 20,0 Z" fill="#C09E5A" opacity="0.6"/>
  </g>

  <!-- World map outline (simplified) -->
  <g opacity="0.15" stroke="#C09E5A" stroke-width="2" fill="none" transform="translate(600, 300)">
    <!-- Simplified continents -->
    <ellipse cx="0" cy="0" rx="250" ry="120"/>
  </g>

  <!-- Title -->
  <text x="100" y="100" font-family="Inter, sans-serif" font-size="48" font-weight="700" fill="#FFFFFF">
    National Security
  </text>
  <text x="100" y="150" font-family="Inter, sans-serif" font-size="48" font-weight="700" fill="#FFFFFF">
    Nightmare
  </text>

  <!-- Subtitle -->
  <text x="100" y="200" font-family="Inter, sans-serif" font-size="20" fill="rgba(255,255,255,0.7)">
    When AI Capabilities Get Stolen,
  </text>
  <text x="100" y="230" font-family="Inter, sans-serif" font-size="20" fill="rgba(255,255,255,0.7)">
    Safety Guardrails Get Stripped
  </text>

  <!-- Threat indicators -->
  <g transform="translate(100, 300)">
    <text x="0" y="0" font-family="JetBrains Mono, monospace" font-size="16" font-weight="600" fill="#FF4444">
      ▸ Bioweapons Design Without Refusal
    </text>
    <text x="0" y="35" font-family="JetBrains Mono, monospace" font-size="16" font-weight="600" fill="#FF4444">
      ▸ Cyberattack Planning Enabled
    </text>
    <text x="0" y="70" font-family="JetBrains Mono, monospace" font-size="16" font-weight="600" fill="#FF4444">
      ▸ Mass Disinformation at Scale
    </text>
    <text x="0" y="105" font-family="JetBrains Mono, monospace" font-size="16" font-weight="600" fill="#FF4444">
      ▸ Autonomous Weapons Development
    </text>
  </g>

  <!-- Warning banner -->
  <rect x="100" y="480" width="600" height="60" fill="rgba(255,68,68,0.15)" stroke="#FF4444" stroke-width="2" rx="5"/>
  <text x="400" y="515" font-family="JetBrains Mono, monospace" font-size="18" font-weight="700" fill="#FF4444" text-anchor="middle">
    FRONTIER MODELS WITHOUT SAFETY CONTROLS
  </text>
</svg>